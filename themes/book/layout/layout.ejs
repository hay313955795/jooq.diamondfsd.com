<!DOCTYPE html>
<html lang="zh-CN">
  <head>
    <title><%- page.title ? page.title + ' - ' : '' %> <%-config.title%></title>
    <meta charset="utf-8">
    <meta name="description" content="<%- config.description %>">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link href="//code.bdstatic.com/npm/font-awesome@4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">

    <!-- main page styles -->
    <%- css('css/page') %>

    <!-- this needs to be loaded before guide's inline scripts -->
    <script src="<%- url_for('/js/vue.js') %>"></script>

    <script>
      Vue.config.productionTip = false
      window.PAGE_TYPE = "<%- page.type %>"
    </script>

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-115185351-2"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'UA-115185351-2');
    </script>

  </head>
  <body class="docs">
    <div id="mobile-bar">
      <a class="menu-button"></a>
      <h3 class="mobile-title"><%-page.title + ' - ' + config.title%></h3>
    </div>
    <%- partial('partials/header') %>
    <div id="main" class="fix-sidebar">
      <%- body %>
    </div>
    <script src="<%- url_for("/js/smooth-scroll.min.js") %>"></script>

    <!-- main custom script for sidebars, version selects etc. -->
    <script src="<%- url_for("/js/css.escape.js") %>"></script>
    <script src="<%- url_for("/js/common.js") %>"></script>

    <!-- search -->
    <link href="//cdn.jsdelivr.net/npm/docsearch.js@2/dist/cdn/docsearch.min.css" rel='stylesheet' type='text/css'>
    <%- css('css/search') %>
    <script src="//cdn.jsdelivr.net/npm/docsearch.js@2/dist/cdn/docsearch.min.js"></script>
    <script>
      var doSearchApiKey = '<%-config.docsearch_api_key%>'
      var doSearchIndexName = '<%-config.docsearch_index_name%>'
      if (doSearchApiKey) {
        docsearch({
          apiKey: doSearchApiKey,
          indexName: doSearchIndexName,
          inputSelector: '#search-query-nav',
          debug: false
        })
      }
    </script>
  </body>
</html>
