<!DOCTYPE html>
<html lang="zh-CN">
  <head>
    <title><%- page.title ? page.title + ' - ' : '' %> <%-config.title%></title>
    <meta charset="utf-8">
    <meta name="description" content="<%- config.description %>">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link href="//code.bdstatic.com/npm/font-awesome@4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">

    <!-- main page styles -->
    <%- css('css/page') %>

    <!-- this needs to be loaded before guide's inline scripts -->
    <script src="<%- url_for('/js/vue.js') %>"></script>

    <script>
      Vue.config.productionTip = false
      window.PAGE_TYPE = "<%- page.type %>"
    </script>

    <!-- ga -->
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

      ga('create', '<%- config.google_analytics %>', '<%- config.root_domain %>');
      ga('send', 'pageview');
    </script>
  </head>
  <body class="docs">
    <div id="mobile-bar">
      <a class="menu-button"></a>
      <h3 class="mobile-title"><%-page.title + ' - ' + config.title%></h3>
    </div>
    <%- partial('partials/header') %>
    <div id="main" class="fix-sidebar">
      <%- body %>
    </div>
    <script src="<%- url_for("/js/smooth-scroll.min.js") %>"></script>

    <!-- main custom script for sidebars, version selects etc. -->
    <script src="<%- url_for("/js/css.escape.js") %>"></script>
    <script src="<%- url_for("/js/common.js") %>"></script>

    <!-- search -->
    <link href="//code.bdstatic.com/npm/docsearch.js@1.5.0/dist/cdn/docsearch.min.css" rel='stylesheet' type='text/css'>
    <%- css('css/search') %>
    <script src="//code.bdstatic.com/npm/docsearch.js@1.5.0/dist/cdn/docsearch.min.js"></script>
    <script>
      var doSearchAppId = '<%-config.docsearch_app_id%>'
      var doSearchApiKey = '<%-config.docsearch_api_key%>'
      var doSearchIndexName = '<%-config.docsearch_index_name%>'
      if (doSearchAppId) {
        [
          '#search-query-nav',
          '#search-query-sidebar',
          '#search-query-menu'
        ].forEach(function (selector) {
          if (!document.querySelector(selector)) return
          // search index defaults to v2
          var match = window.location.pathname.match(/^\/(v\d+)/)
          docsearch({
            appId: doSearchAppId,
            apiKey: doSearchApiKey,
            indexName: doSearchIndexName,
            inputSelector: selector,
            autocompleteOptions: { hint: false, appendTo: 'body'}
          })
        })
      }
    </script>
  </body>
</html>
